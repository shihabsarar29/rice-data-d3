n      <ol>\n        <li>Bar Chart showing the Applicants number for first-time degree-seeking undergraduates</li>\n        <li>Bar Chart showing the Matriculants number for first-time degree-seeking undergraduates</li>\n        <li>Pie Chart showing the enrollment of students by Race/Ethnicity</li>\n        <li>Pie Chart showing the enrollment of students by Geographic Origin</li>\n        <li>Line Chart displaying the total budget for the last 10 years</li>\n        <li>Line Chart displaying the end-of-fiscal year sponsored research revenues</li>\n      </ol>\n\n    </div>\n  );\n}\n","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/RaceChart/RaceChartPage.js",["239"],[],"import React from 'react';\nimport RaceChart from \"./RaceChart\";\n\nconst data = [\n  { label: \"Asian\", value: 27 },\n  { label: \"White\", value: 41 },\n  { label: \"Other\", value: 6 },\n  { label: \"Hispanic\", value: 15 },\n  { label: \"Black\", value: 11 },\n]\n\nexport default function RaceChartPage() {\n  return (\n\n    <div style={{ width: \"100%\", margin: \"5vh\" }}>\n      <h1>Race/Ethnicity</h1>\n      <RaceChart data={data} />\n      <p>A Pie Chart showing the enrollment of students by Race/Ethnicity\n        <a href=\"https://oie.rice.edu/IR-reporting/student-enrollment\" target=\"_blank\">(Data Source)</a>\n      </p>\n    </div>\n  )\n}\n","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/MatriculantChart/MatriculantChartPage.js",["240"],[],"import React from 'react';\nimport MatriculantChart from \"./MatriculantChart\";\n\nconst data = [\n  {\n    Year: 2012,\n    Number: 2528\n  },\n  {\n    Year: 2013,\n    Number: 2583\n  },\n  {\n    Year: 2014,\n    Number: 2677\n  },\n  {\n    Year: 2015,\n    Number: 2865\n  },\n  {\n    Year: 2016,\n    Number: 2785\n  },\n  {\n    Year: 2017,\n    Number: 2864\n  },\n  {\n    Year: 2018,\n    Number: 2328\n  },\n  {\n    Year: 2019,\n    Number: 2361\n  },\n  {\n    Year: 2020,\n    Number: 2555\n  },\n  {\n    Year: 2021,\n    Number: 2802\n  },\n]\n\nexport default function MatriculantChartPage() {\n  return (\n    <div style={{ width: \"100%\", margin: \"5vh\" }}>\n      <h1>Matriculants</h1>\n      <MatriculantChart data={data} />\n      <h4>X-axis: Year<br></br>\n        Y-axis: Number of Matriculants</h4>\n      <p>A Bar Chart showing the Matriculants number for first-time degree-seeking undergraduates\n        <a href=\"https://oie.rice.edu/IR-reporting/undergraduate-admissions-statistics\" target=\"_blank\">(Data Source)</a>\n      </p>\n    </div>\n  )\n}\n","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/BudgetChart/BudgetChartPage.js",["241"],[],"\nimport React, { Component } from 'react';\nimport { budgetStock as budgetData } from './BudgetData';\nimport BudgetChart from './BudgetChart.jsx';\nimport * as _ from 'lodash';\n\nclass BudgetChartPage extends Component {\n\n  render() {\n    let content1;\n    content1 = <BudgetChart data={_.cloneDeep(budgetData)} />;\n\n    return (\n      <div style={{ width: \"100%\", margin: \"5vh\" }}>\n        <h1>Consolidated Budget</h1>\n        {content1}\n        <h4>X-axis: Year<br></br>\n          Y-axis: Budget Amount</h4>\n        <p>A Line Chart displaying the total budget for the last 10 years.\n          <a href=\"https://oie.rice.edu/IR-reporting/consolidated-budget\" target=\"_blank\">(Data Source)</a>\n        </p>\n      </div>\n    );\n  }\n}\n\nexport default BudgetChartPage;\n","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/GeographicChart/GeographicChartPage.js",["242"],[],"import React from 'react';\nimport GeographicChart from \"./GeographicChart\";\n\nconst data = [\n  { label: \"Texas\", value: 38 },\n  { label: \"International\", value: 25 },\n  { label: \"Out-of-State\", value: 36 },\n]\n\nexport default function GeographicChartPage() {\n  return (\n\n    <div style={{ width: \"100%\", margin: \"5vh\" }}>\n      <h1>Geographic Origin</h1>\n      <GeographicChart data={data} />\n      <p>A Pie Chart showing the enrollment of students by Geographic Origin\n        <a href=\"https://oie.rice.edu/IR-reporting/student-enrollment\" target=\"_blank\">(Data Source)</a>\n      </p>\n    </div>\n  )\n}\n","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/ApplicantChart/ApplicantChart.js",[],[],"/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/ResearchChart/ResearchChartPage.js",["243"],[],"\nimport React, { Component } from 'react';\nimport { researchStock as researchData } from './ResearchData';\nimport ResearchChart from './ResearchChart.jsx';\nimport * as _ from 'lodash';\n\nclass ResearchChartPage extends Component {\n\n  render() {\n    let content1;\n    content1 = <ResearchChart data={_.cloneDeep(researchData)} />;\n\n    return (\n      <div style={{ width: \"100%\", margin: \"5vh\" }}>\n        <h1>Sponsored Research</h1>\n        {content1}\n        <h4>X-axis: Year<br></br>\n          Y-axis: Funding Amount</h4>\n        <p>A Line Chart displaying the end-of-fiscal year sponsored research revenues\n          <a href=\"https://oie.rice.edu/IR-reporting/sponsored-research\" target=\"_blank\">(Data Source)</a>\n        </p>\n      </div>\n\n    );\n  }\n}\n\nexport default ResearchChartPage;\n","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/BudgetChart/BudgetChart.js",[],[],"/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/ResearchChart/ResearchChart.jsx",[],[],"/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/RaceChart/RaceChart.js",["244"],[],"import React, { useEffect } from 'react';\nimport * as d3 from 'd3';\n\nfunction RaceChart(props) {\n  const {\n    data,\n  } = props;\n\n  const margin = {\n    top: 30, right: 0, bottom: 0, left: 0,\n  };\n\n  const width = 2 * 200 + margin.left + margin.right;\n  const height = 2 * 200 + margin.top + margin.bottom;\n\n  const colorScale = d3\n    .scaleSequential()\n    .interpolator(d3.interpolateCool)\n    .domain([0, data.length]);\n\n  useEffect(() => {\n    drawChart();\n  }, [data]);\n\n  function drawChart() {\n    // Remove the old svg\n    d3.select('#pie-container')\n      .select('svg')\n      .remove();\n\n    // Create new svg\n    const svg = d3\n      .select('#pie-container')\n      .append('svg')\n      .attr('width', width)\n      .attr('height', height)\n      .append('g')\n      .attr('transform', `translate(${width / 2}, ${height / 2})`);\n\n    const arcGenerator = d3\n      .arc()\n      .innerRadius(0)\n      .outerRadius(200);\n\n    const pieGenerator = d3\n      .pie()\n      .padAngle(0)\n      .value((d) => d.value);\n\n    const arc = svg\n      .selectAll()\n      .data(pieGenerator(data))\n      .enter();\n\n    // Append arcs\n    arc\n      .append('path')\n      .attr('d', arcGenerator)\n      .style('fill', (_, i) => colorScale(i))\n      .style('stroke', '#ffffff')\n      .style('stroke-width', 0);\n\n    // Append text labels\n    arc\n      .append('text')\n      .attr('text-anchor', 'middle')\n      .attr('alignment-baseline', 'middle')\n      .text((d) => d.data.label)\n      .style('fill', (_, i) => colorScale(data.length - i))\n      .attr('transform', (d) => {\n        const [x, y] = arcGenerator.centroid(d);\n        return `translate(${x}, ${y})`;\n      });\n  }\n\n  return <div id=\"pie-container\" />;\n}\n\nexport default RaceChart;","/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/BudgetChart/BudgetData.js",[],[],"/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/MatriculantChart/MatriculantChart.js",[],[],"/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/GeographicChart/GeographicChart.js",["245"],[],"/Users/shihab/StudioProjects/rice-data-d3/rice-d3/src/components/ApplicantChart/ApplicantChartPage.js",["246"],[],"import React from 'react';\nimport ApplicantChart from \"./ApplicantChart\";\n\nconst data = [\n  {\n    Year: 2012,\n    Number: 15133\n  },\n